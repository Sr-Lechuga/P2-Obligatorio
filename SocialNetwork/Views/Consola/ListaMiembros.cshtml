@using Dominio.Entidades;

@{
    ViewBag.Title = "Listado Miembros";
}

@if (ViewBag.MensajeError != null)
{
    <h2 class="alert-danger text-center">@ViewBag.MensajeError</h2>
}
@if (TempData["Mensaje"] != null)
{
    <h2 class="alert-success text-center">@TempData["Mensaje"]</h2>
}

@model List<Miembro>
@* Lista con todos los miembros en el sistema *@

@*

//Esto es para probar el back, borrar despues

*@

@foreach (Miembro miembro in Model)
{
    if (miembro.Bloqueado)
    {
        <p>@miembro.ToString()</p>
    }
    else
    {
        <p><a href="/Consola/BloquearMiembro?emailMiembro=@miembro.Email">@miembro.ToString()</a></p>
    }
}